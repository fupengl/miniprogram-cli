{
  "name": "wapp-cli",
  "version": "0.0.80",
  "description": "微信小程序cli",
  "main": "bin/index.js",
  "bin": {
    "mpc": "bin/mpc"
  },
  "scripts": {
    "test:serve": "cd test && ENV=development CONF=dev ../bin/mpc serve",
    "test:build": "cd test && ENV=development CONF=build GENERATE_SOURCEMAP=false ../bin/mpc build",
    "prebuild": "rimraf ./lib && mkdirp ./lib",
    "prepublish": "npm run build",
    "build:watch": "babel src -o lib/index.js -w",
    "build": "npx babel src --out-dir lib",
    "lint": "eslint --ext .js command/ --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fupengl/miniprogram-cli.git"
  },
