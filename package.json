    "miniprogram",
    "wxapp-cli",
    "weapp-cli",
    "cli"
  ],
  "author": "fupengl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fupengl/miniprogram-cli/issues"
  },
  "homepage": "https://github.com/fupengl/miniprogram-cli#readme",
